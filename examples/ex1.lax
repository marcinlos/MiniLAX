PROGRAM ex1;

DECLARE
  fib : ARRAY [1..10] OF INTEGER;
  PROCEDURE F(n : INTEGER);
  DECLARE
    k: INTEGER;
    PROCEDURE dupa(VAR n : BOOLEAN; kk : INTEGER);
    DECLARE
      j: INTEGER;
      gg: ARRAY [4..9] OF BOOLEAN
    BEGIN
      k := 9
    END
  BEGIN
    fib[1] := 1;
    fib[2] := 1;
    WHILE k < n DO
      fib[k+2] := fib[k + 1] + fib[k]
    END
  END
BEGIN
  F(3)
END.

